<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    
    <div class="container">
        <header>
            @{
                Html.RenderPartial("Header_PartialPage");
                Html.RenderPartial("Menu_PartialPage");
            }
        </header>
        <div class="row">
            <div class="col-10">
                <div>
                    @{
                        Html.RenderPartial("Slide_PartialPage");
                    }
                </div>
                <div style="min-height: 180px;">
                    @RenderBody()
                </div>
            </div>
            <div class="col-2">
                @{
                    Html.RenderPartial("SlideRight_PartialPage");
                }
            </div>
        </div>
        <footer>
            @{
                Html.RenderPartial("Footer_PartialPage");
            }
        </footer>
    </div>
    
</body>
</html>
